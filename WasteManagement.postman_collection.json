		{
			"name": "wasteadminregister",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Test that response status code is 200",
							"pm.test(\"Verify Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Parse response JSON",
							"let jsonData;",
							"try {",
							"    jsonData = pm.response.json();",
							"} catch (e) {",
							"    jsonData = {};",
							"}",
							"",
							"// Test that message property exists and is 'Registration successful'",
							"pm.test(\"Verify Response has message 'Registration successful'\", function () {",
							"    pm.expect(jsonData).to.have.property('message', 'Registration successful');",
							"});",
							"",
							"// Test that userdetails is an array with at least one user object",
							"pm.test(\"Verify Response has userdetails array with at least one user\", function () {",
							"    pm.expect(jsonData).to.have.property('userdetails').that.is.an('array').with.length.greaterThan(0);",
							"});",
							"",
							"// Test that the first user object has expected properties",
							"pm.test(\"Verify User object has expected properties\", function () {",
							"    const user = jsonData.userdetails[0];",
							"    pm.expect(user).to.be.an('object');",
							"    pm.expect(user).to.have.property('id');",
							"    pm.expect(user).to.have.property('fullname');",
							"    pm.expect(user).to.have.property('phone');",
							"    pm.expect(user).to.have.property('email');",
							"    pm.expect(user).to.have.property('password');",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
